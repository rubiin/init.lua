{
  "diagnostics.globals": [
    "vim",
    "log",
    "cmp_mappings",
    "vim",
    "jit",
    "LazyVim",
    "Snacks"
  ],
  "runtime.version": "LuaJIT",
  "diagnostics.disable": [
    "unused-local",
    "different-requires",
    "undefined-field",
    "redundant-parameter",
    "undefined-doc-name",
    "redefined-local",
    "need-check-nil",
    "no-unknown"
  ],
  "workspace.checkThirdParty": false
}
